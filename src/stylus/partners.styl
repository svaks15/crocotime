.main_footer
  display flex
  flex-direction row
  min-width 1200px
  font-size 14px
  color #ffffff
  background #1a242f
  padding 40px 100px
  margin 0 auto
  .contacts_block
    display inline-block
    vertical-align top
    width 43.2%
    min-width 295px
    .contacts_block_logo
      margin-bottom 25px
      a
        font-size 30px
        color #ffffff
    .contacts_block_content
      display block
      line-height 2.143
      margin-bottom 35px
      .contacts_block_content_left
        display inline-block
        vertical-align top
        min-width 80px
      .contacts_block_content_right
        display inline-block
        vertical-align top
      .contacts_block_content_right_email
        font-size 14px
        color #6777bd
      .contacts_block_content_right_skype
        font-size 14px
        color #6777bd
      a
        color #6777bd
        text-decoration none
        &:hover
          color #fff

    .indent
      margin-bottom 50px

  .footer_menu_block
    display inline-block
    vertical-align top
    width 14.2%
    min-width 108px
    margin-top 5px
    .footer_menu_block_header
      font-size 18px
    .footer_menu_block_item
      font-size 14px
      color #6777bd
      margin-top 30px
      padding 0
      font-weight 300
      a
        color #6777bd
        text-decoration none
        &:hover
          color #fff

    .indent_item
      margin-top 40px
    .footer_languages
      display flex
      flex-direction row
      .ru_icon
        width 20px
        height 14px
        margin 4px 20px 0 0
        background url("../img/ru_icon.png") no-repeat
      .en_icon
        width 20px
        height 14px
        margin 4px 20px 0 0
        background url("../img/en_icon.png") no-repeat

