            .footer_menu_block
                .footer_menu_block_header О КОМПАНИИ
                .footer_menu_block_item.indent_item
                    a(href="#") О нас
                .footer_menu_block_item
                    a(href="#") Партнерство
                .footer_menu_block_item
                    a(href="#") Блог
                .footer_menu_block_item
                    a(href="#") Пресса о нас
                .footer_menu_block_item
                    a(href="#") Конфиденциальность
                .footer_menu_block_item
                    a(href="#") Лицензионное<br>соглашение

            .footer_menu_block
                .footer_menu_block_header ЯЗЫК САЙТА
                .footer_menu_block_item.indent_item.footer_languages
                    .ru_icon
                    a(href="#") Русский
                .footer_menu_block_item.footer_languages
                    .en_icon
                    a(href="#") Английский
